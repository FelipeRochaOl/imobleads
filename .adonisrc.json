{
  "typescript": true,
  "commands": [
    "./commands",
    "@adonisjs/core/build/commands",
    "@adonisjs/repl/build/commands",
    "@adonisjs/lucid/build/commands",
    "@adonisjs/bouncer/build/commands"
  ],
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "app",
    "Config": "config",
    "Database": "database",
    "Contracts": "contracts"
  },
  "preloads": [
    "./start/routes",
    "./start/kernel",
    {
      "file": "./start/events",
      "environment": ["console", "repl", "web"]
    },
    {
      "file": "./start/redis",
      "environment": ["console", "repl", "web"]
    },
    "./start/bouncer"
  ],
  "providers": [
    "./providers/AppProvider",
    "@adonisjs/core",
    "@adonisjs/lucid",
    "@adonisjs/auth",
    "@adonisjs/redis",
    "@adonisjs/bouncer"
  ],
  "aceProviders": ["@adonisjs/repl"],
  "namespaces": {
    "httpControllers": "App/Modules",
    "httpMiddlewares": "App/Modules",
    "httpModels": "App/Modules",
    "policies": "App/Modules"
  }
}
